{
  ".grow": {
    "&-relative": {
      "height": null,
      "width": null,
      "flexProps": {
        "flexGrow": 1
      }
    },
    "&-absolute": {
      "flexProps": {
        "positionType": "ABSOLUTE"
      },
      "height": null,
      "width": null,
      "left": 0,
      "right": 0,
      "bottom": 0,
      "top": 0
    },
    "flexProps": {
      "flexGrow": 1
    }
  },
  ".padding": {
    "&-vertical": {
      "paddingBottom": 15,
      "paddingTop": 15,
      "backgroundColor": "rgba( 208, 2, 27, 1 )"
    },
    "&-horizontal": {
      "paddingLeft": 15,
      "paddingRight": 15
    }
  },
  ".margin": {
    "&-vertical": {
      "marginBottom": 15,
      "marginTop": 15
    },
    "&-horizontal": {
      "marginLeft": 15,
      "marginRight": 15
    }
  },
  ".shadow": {
    "ios": {
      "shadowOffset": {
        "x": 2,
        "y": 3
      },
      "shadowRadius": 1,
      "shadowOpacity": 0.4,
      "masksToBounds": false
    },
    "android": {
      "elevation": 5
    }
  },
  ".hidden": {
    "width": 0,
    "height": 0,
    "marginBottom": 0,
    "marginTop": 0,
    "marginLeft": 0,
    "marginRight": 0,
    "padding": 0,
    "paddingBottom": 0,
    "paddingLeft": 0,
    "paddingRight": 0,
    "paddingTop": 0
  },
  ".absolute": {
    ".topleft": {
      "height": null,
      "left": 0,
      "top": 0
    },
    ".topright": {
      "height": null,
      "top": 0,
      "right": 0
    },
    ".bottomleft": {
      "height": null,
      "bottom": 0,
      "left": 0
    },
    ".bottomright": {
      "height": null,
      "bottom": 0,
      "right": 0
    },
    "flexProps": {
      "positionType": "ABSOLUTE"
    }
  },
  ".centerChild": {
    "flexProps": {
      "justifyContent": "CENTER"
    }
  },
  ".bgBlack": {
    "backgroundColor": "rgba( 0, 0, 0, 1 )"
  },
  ".abst": {
    "width": 100,
    "height": 100,
    "backgroundColor": "rgba( 126, 211, 33, 1 )",
    "top": 85,
    "left": 60,
    "flexProps": {
      "positionType": "ABSOLUTE"
    }
  },
  ".bgRed": {
    "backgroundColor": "rgba( 208, 2, 27, 1 )"
  },
  ".abst1": {
    "flexProps": {
      "positionType": "ABSOLUTE"
    },
    "width": 100,
    "height": 100,
    "backgroundColor": "rgba( 80, 227, 194, 1 )",
    "left": 100,
    "top": 100
  },
  ".bgGreen": {
    "backgroundColor": "rgba( 126, 211, 33, 1 )"
  },
  ".abst2": {
    "flexProps": {
      "positionType": "ABSOLUTE"
    },
    "width": 80,
    "height": 80,
    "left": 200,
    "top": 200,
    "bottom": 0,
    "backgroundColor": "rgba( 208, 2, 27, 1 )"
  },
  ".abst3": {
    "flexProps": {
      "positionType": "ABSOLUTE"
    },
    "width": 80,
    "height": 80,
    "left": 100,
    "top": 200,
    "bottom": 0,
    "backgroundColor": "rgba( 248, 231, 28, 1 )"
  }
}